{
  "manifest_version": 2,
  "name": "COBI - jetpack simulator",
  "version": "0.0.0.1",

  "description": "COBI's webapp events simulator",
  "minimum_chrome_version": "43",

  "browser_action": {
    "default_icon": "assets/cobi-icon.png",
    "default_title": "COBI simulator",
    "default_popup": "popup.html"
  },

  "icons": {
    "16": "assets/cobi-icon.png",
    "48": "assets/cobi-icon.png",
    "128": "assets/cobi-icon.png"
  },

  "devtools_page": "index.html",
  "homepage_url": "https://github.com/cobi-bike/jetpack-simulator",

  "offline_enabled": true
}
