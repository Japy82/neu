{
  "name": "jetpack-simulator",
  "version": "0.1.0",
  "description": "COBI simulator to help you test your Jetpack app",
  "scripts": {
    "test": "flow && eslint",
    "build": "flow && gulp default && npm run docs",
    "prepublish": "npm run build",
    "flow": "flow",
    "gulp": "gulp",
    "docs": "documentation build -f html -o ./docs -g --shallow ./app/js/**.js"
  },
  "author": "COBI GmbH",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-env": "^1.4.0",
    "babel-preset-flow": "^6.23.0",
    "babelify": "^7.3.0",
    "browserify": "^14.3.0",
    "documentation": "^4.0.0-rc.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-flowtype": "^2.32.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "flow-bin": "^0.44.2",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.9.0"
  },
  "keywords": [
    "COBI",
    "jetpack",
    "simulator",
    "chrome",
    "devtools"
  ]
}
